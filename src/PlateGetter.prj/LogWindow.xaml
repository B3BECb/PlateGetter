<Window x:Class="PlateGetter.LogWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PlateGetter"
        mc:Ignorable="d"
        Title="Log" Height="350" Width="525" WindowStyle="ToolWindow" Closing="Window_Closing">
    <Grid>
        <ScrollViewer>
            <TextBlock x:Name="textBlock" TextWrapping="WrapWithOverflow" Margin="10"/>
        </ScrollViewer>
    </Grid>
</Window>
